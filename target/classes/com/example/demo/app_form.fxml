<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="15" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ApplicationController">

    <padding>
        <Insets topRightBottomLeft="15" />
    </padding>

    <DatePicker fx:id="picker" />

    <Button onAction="#updateNotes" text="Update Notes" />

    <TextArea fx:id="notes" />
   <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteUser" text="Delete user" textFill="#fc0707" translateX="180.0" translateY="40.0">
      <opaqueInsets>
         <Insets />
      </opaqueInsets></Button>
   <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logout" text="Sign out" translateX="-150.0" />

</VBox>
